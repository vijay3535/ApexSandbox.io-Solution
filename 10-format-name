public String formatName(String firstName, String lastName) {
    //code here
    if(String.isBlank(firstName) && String.isBlank(lastName)) {
   return '';
    }
 else if(String.isBlank(lastName)){
    return 'firstName';

} else if (String.isBlank(firstName)){
    return 'lastName';
	}
else{
    return LastName + ' ,' LastName;

}
