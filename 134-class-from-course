public Id classFromCourse(String courseName) {
    // code here
List<apxio__Course__c> course = [select id,Name
 from apxio__Course__c where 
 apxio__Course__c.Name =: courseName]]

if(course.size()=0){
    return null;
}

apxio__Course__c coursest = new apxio__Course__c();
coursest.apxio__Course__c=course[0].id;
coursest.name=course[0].name;

insert coursest;

}