public String registerStudent(String name, String phone, String email) {
    // code here
     apxio__Student__c student = new apxio__Student__c();
      student.Name=name;
       student.apxio__Phone__c=phone;
      student.apxio__Email__c=email;
       insert student;

   List<apxio__Student__c> s1 =[select name,apxio__Registration_Number__c from apxio__Student__c where id=:student.id ];

   return  s1.apxio__Registration_Number__c;


}